import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int i;
	   
	    System.out.println("enter n");
	    Scanner sc = new Scanner(System.in);
	    int n =sc.nextInt();
	    int arr1[]= new int[n];
	
	    System.out.print("enter the elements of the array");
	    for( i=0;i<n;i++){
	         arr1[i] =sc.nextInt();
	    }
	    
for(i=0;i<n;i++)
{
  if((i+1)%2==0)  
  {
     System.out.print(arr1[i]) ;
  }
}
	    
	}
}
